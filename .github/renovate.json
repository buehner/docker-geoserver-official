{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        "docker:pinDigests"
    ],
    "forkProcessing": "enabled",
    "autodiscover": true,
    "autodiscoverFilter": [
        "buehner/docker-geoserver-official"
    ],
    "packageRules": [
        {
            "matchUpdateTypes": [
                "minor",
                "patch",
                "pin",
                "digest"
            ]
        }
    ]
}